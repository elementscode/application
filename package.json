{
  "name": "@elements/application",
  "description": "The elements web framework.",
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE.txt",
  "browser": {
    "./app-middleware": false,
    "./asset-middleware": false,
    "./body-middleware": false,
    "./email": false,
    "./helpers": "./helpers-browser",
    "./middleware-stack": false,
    "./migration": false,
    "./server": false,
    "./server-request": false,
    "./server-session": "./browser-session",
    "./session-middleware": false,
    "./start": "./start-browser",
    "./types": false,
    "./html": false
  },
  "dependencies": {
    "@elements/compiler": ">=0.0.0 <0.1.0",
    "@elements/config": ">=0.0.0 <0.1.0",
    "@elements/error": ">=0.0.0 <0.1.0",
    "@elements/json": ">=0.0.0 <0.1.0",
    "@elements/parse": ">=0.0.0 <0.1.0",
    "@elements/postgres": ">=0.0.0 <0.1.0",
    "@elements/runtime": ">=0.0.0 <0.1.0",
    "@elements/utils": ">=0.0.0 <0.1.0",
    "base64-url": ">=2.3.3 <2.4.0",
    "debug": ">=4.1.1 <4.2.0",
    "mime": ">=2.4.6 <2.5.0",
    "nodemailer": ">=6.4.14 <6.5.0",
    "path-to-regexp": ">=6.1.0 <6.2.0",
    "url-parse": ">=1.4.7 <1.5.0",
    "uuid": ">=8.1.0 <8.2.0",
    "ws": ">=7.3.1 <7.4.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0 <16.15.0",
    "react-dom": ">=16.14.0 <16.15.0"
  },
  "devDependencies": {
    "@types/node": ">=14.11.11 <14.12.0",
    "@types/react": ">=16.9.53 <16.10.0",
    "@types/react-dom": ">=16.9.8 <16.10.0"
  },
  "scripts": {
    "clean": "rm -rf .elements/dist",
    "copyfiles": "mkdir -p .elements/dist && cp README.md LICENSE.txt package.json .elements/dist",
    "build": "yarn run clean && yarn run copyfiles && tsc",
    "build:watch": "yarn run clean && yarn run copyfiles && tsc --watch"
  }
}
